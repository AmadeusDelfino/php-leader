{
  "name": "adelf/leader-server",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "amadeusdelfino/laravel-config-standalone": "^1.2",
    "clue/stdio-react": "^2.3",
    "guzzlehttp/guzzle": "^6.5",
    "pimple/pimple": "^3.2",
    "psr/cache": "^1.0",
    "psr/log": "^1.1",
    "react/event-loop": "^1.1",
    "react/http": "^0.8.6"
  },
  "autoload": {
    "psr-4": {
      "ADelf\\LeaderServer\\": "src/",
      "ADelf\\LeaderServer\\Contracts\\": "contracts/src/",
      "ADelf\\LeaderServer\\WorkerActions\\": "worker_actions/src/",
      "ADelf\\Processor\\": "processor/src/",
      "ADelf\\LeaderWorker\\Contracts\\": "worker_contracts/src/",
      "ADelf\\LeaderWorker\\": "worker/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },

  "require-dev": {
    "phpunit/phpunit": "^9"
  }
}
